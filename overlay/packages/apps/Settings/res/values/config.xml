<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
     <!-- For Ultra Power Saving -->
     <bool name="config_supportUltraPowerSaving">true</bool>

     <!-- For Enabling Battery Heath -->
     <bool name="config_supportBatteryHealth">true</bool>

     <!-- Battery Health -->
     <string name="config_batDesCap">/sys/class/power_supply/bms/charge_full</string>
     <string name="config_batCurCap">/sys/class/power_supply/bms/charge_now_raw</string>
     <string name="config_batChargeCycle">/sys/class/power_supply/bms/cycle_count</string>

    <!-- Whether SELinux status should be shown or not. -->
    <bool name="config_show_selinux_status">true</bool>

    <!-- Smart Charging -->
    <bool name="config_supportSmartCharging">true</bool>

    <!-- Whether or not the device is capable of multiple levels of vibration intensity.
         Note that this is different from whether it can control the vibration amplitude as some
         devices will be able to vary their amplitude but do not possess enough dynamic range to
         have distinct intensity levels -->
    <bool name="config_vibration_supports_multiple_intensities">true</bool>
</resources>
